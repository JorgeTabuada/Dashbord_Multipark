{
  "name": "dashbord-multipark",
  "version": "2.0.0",
  "description": "Dashboard Multipark com integração multi-database Supabase",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:apis": "node scripts/test-apis.js",
    "test:excel": "node scripts/test-excel-upload.js",
    "test:all": "npm run test:apis && npm run test:excel",
    "db:types": "supabase gen types typescript --local > lib/supabase/types/generated.ts",
    "db:sync": "curl -X POST http://localhost:3000/api/sync/parques",
    "legacy:serve": "python -m http.server 8080",
    "legacy:open": "open http://localhost:8080"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.45.4",
    "@supabase/ssr": "^0.4.0",
    "next": "^15.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.4",
    "@types/node": "^20.14.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "swr": "^2.2.5",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.427.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.2",
    "class-variance-authority": "^0.7.0",
    "recharts": "^2.12.7",
    "react-hook-form": "^7.52.2",
    "@hookform/resolvers": "^3.9.0",
    "zod": "^3.23.8",
    "sonner": "^1.5.0",
    "cmdk": "^1.0.0",
    "react-select": "^5.8.0",
    "react-datepicker": "^7.3.0",
    "framer-motion": "^11.3.28",
    "react-hot-toast": "^2.4.1",
    "formidable": "^3.5.1",
    "exceljs": "^4.4.0",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.14",
    "tailwindcss": "^3.4.10",
    "postcss": "^8.4.41",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.1.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.9",
    "jest": "^29.7.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/jest-dom": "^6.4.8",
    "jest-environment-jsdom": "^29.7.0",
    "supabase": "^1.188.6",
    "@types/formidable": "^3.4.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JorgeTabuada/Dashbord_Multipark.git"
  },
  "keywords": [
    "dashboard",
    "multipark",
    "supabase",
    "nextjs",
    "typescript",
    "parking",
    "management",
    "multi-database"
  ],
  "author": "Jorge Tabuada",
  "license": "MIT"
}